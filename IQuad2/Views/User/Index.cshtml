@model IEnumerable<IQuad2.Models.ApplicationUser>

@{
    ViewBag.Title = "user";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Users</h2>


<table id="user" class="table table-bordered table-hover">
    <thead>

        <tr>
           
            <th>First Name</th>
            <th>First Lame</th>
            <th>User Type</th>


        </tr>

    </thead>
    <tbody>
        @foreach (var users in Model)
        {
            <tr>
                @*<td>@Html.ActionLink("<==>", "Edit", "Appointment", new { id = users.Id }, null) </td>*@
                <td>@users.Fname</td>
                <td>@users.Lname</td>
                <td>@users.UserType.Name</td>
            </tr>
        }

    </tbody>
</table>

